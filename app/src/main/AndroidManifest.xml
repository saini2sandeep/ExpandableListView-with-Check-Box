<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sandeepsaini.expandablelistview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".simple_el.ExpandableListActivity" />
        <activity android:name=".data_preview.SelectedDataPreviewActivity" />
        <activity android:name=".with_header_cb.CheckBoxExpandableListActivity" />
        <activity android:name=".without_header_cb.ExpandableListWithChildCBSelection"></activity>
    </application>

</manifest>